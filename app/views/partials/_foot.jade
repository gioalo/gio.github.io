//- footer
footer.footer.u-pad-top
  .container
      //- .group
        .grid-2
          .brand-logo_grey
            a(href="#home" aria-label="logo") 
                  svg
                    use(xlink:href="#logo-grey")
      .grid-4
        span.footer--blurb Say hi @
        ul.u-list-inline.footer--social
          each link in links
            li 
              a(style="color:white" href=link.href aria-label=link.id) 
                svg
                  use(xlink:href="#" + link.id)
      .grid-4
        p.footer--legal
          | 
          <strong>2018 #{author}</strong>
          | . All Rights Reserved.
  
